{
	"id": "interstitial",
	"name": "Interstitial Overlay",
	"version": "0.0.1",
	"class": "Interstitial",
	"file": "interstitial.py",
	"module": "python.plugins.interstitial.interstitial",
	"description": "Interrupts the current display with an image.",
	"disabled": false,
	"features": ["layer-priority", "layer-overlay", "media-image"],
	"settings": {
		"schema": {
			"properties": [
			]
		},
		"default": null
