				}
			},
			"properties": [
				{
					"name": "dataSource",
					"type": "schema",
					"required": true,
					"default": null,
					"lookup": "dataSource",
					"label": "Data Source",
					"description": "Single image source."
				}
			]
		},
		"default": {
			"dataSource": null
		}
	}
